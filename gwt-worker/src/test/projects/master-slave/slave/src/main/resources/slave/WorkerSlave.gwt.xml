<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google, Inc.//DTD GWT Module Descriptor//EN" "https://github.com/gwtproject/gwt/raw/2.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="WorkerSlave">
	<!-- some basic inherits -->
	<inherits name="com.google.gwt.user.User" />

	<!-- required in slave -->
	<inherits name="com.github.gwt.worker.WorkerSlave" />
	
	<!-- optional, by default uses SEVERE -->
	<inherits name="com.github.gwt.worker.logging.WorkerLogging" />
	<!-- optional, switch to INFO and lower -->
	<set-property name="gwt.logging.enabled" value="TRUE"/>
	
	<entry-point class="slave.client.WorkerSlaveEntry" />
</module>