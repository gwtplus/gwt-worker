<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google, Inc.//DTD GWT Module Descriptor//EN" "https://github.com/gwtproject/gwt/raw/2.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="Slave">

	<!-- optional, by default uses SEVERE, place where it is added matters! -->
	<inherits name="org.gwtproject.gwt.worker.logging.WorkerLogging" />
	<!-- optional, switch to INFO and lower -->
	<set-property name="gwt.logging.enabled" value="TRUE"/>
	
	
	<inherits name="slave.SlaveBase" />
</module>