<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google, Inc.//DTD GWT Module Descriptor//EN" "https://github.com/gwtproject/gwt/raw/2.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="SlaveSdm">
	<inherits name="slave.SlaveBase" />

	<!-- optional, by default uses SEVERE -->
	<inherits name="com.google.gwt.logging.Logging" />
	<!-- optional, switch to INFO and lower -->
	<set-property name="gwt.logging.enabled" value="TRUE"/>
	
	<collapse-all-properties value="false" />
	<add-linker name="xsworker"/>
</module>